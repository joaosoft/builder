{
  "builder": {
    "source": "main/main.go",
    "destination": "bin/builder",
    "reload_time": 5,
    "log": {
      "level": "info"
    }
  },
  "watcher": {
    "reload_time": 5,
    "dirs": {
      "watch":[ "." ],
      "excluded":[ "vendor", "bin" ],
      "extensions": [ "go", "json", "yml" ]
    },
    "log": {
      "level": "info"
    }
  },
  "manager": {
    "log": {
      "level": "info"
    }
  }
}
